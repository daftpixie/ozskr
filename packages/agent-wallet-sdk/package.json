{
  "name": "@ozskr/agent-wallet-sdk",
  "version": "0.1.2-beta",
  "description": "Non-custodial AI agent wallets on Solana with SPL delegation and budget enforcement",
  "license": "MIT",
  "author": {
    "name": "VT Infinite, Inc",
    "email": "matthew@vt-infinite.com",
    "url": "https://ozskr.ai"
  },
  "bugs": {
    "url": "https://github.com/daftpixie/ozskr/issues",
    "email": "matthew@vt-infinite.com"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./keypair": {
      "import": "./dist/keypair.js",
      "types": "./dist/keypair.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/daftpixie/ozskr.git",
    "directory": "packages/agent-wallet-sdk"
  },
  "keywords": [
    "solana",
    "ai-agent",
    "wallet",
    "delegation",
    "spl-token",
    "non-custodial",
    "mcp",
    "x402",
    "keypair",
    "encryption"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "prepublishOnly": "node -e \"const ua=process.env.npm_config_user_agent||'';if(!ua.includes('pnpm')){const p=require('./package.json');const d=JSON.stringify(p.dependencies||{});if(d.includes('workspace:')){console.error('ERROR: workspace: protocol in dependencies. Use pnpm publish, not npm publish.');process.exit(1)}}\" && pnpm run build && pnpm run test"
  },
  "dependencies": {
    "@solana-program/token": "^0.9.0",
    "@solana/kit": "^2.1.0",
    "@turnkey/sdk-server": "^5.0.3"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "vitest": "^4.0.0"
  }
}
