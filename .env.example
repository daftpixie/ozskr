# ozskr.ai Environment Variables
# Copy to .env.local and fill in values

# Solana RPC (Helius recommended)
NEXT_PUBLIC_HELIUS_RPC_URL=https://devnet.helius-rpc.com/?api-key=YOUR_KEY
NEXT_PUBLIC_SOLANA_NETWORK=devnet

# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://YOUR_PROJECT.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Anthropic (Claude API)
ANTHROPIC_API_KEY=sk-ant-YOUR_KEY

# fal.ai (image/video generation)
FAL_KEY=your-fal-key

# Mem0 (AI memory)
MEM0_API_KEY=your-mem0-key

# Infisical (secrets management)
INFISICAL_SITE_URL=https://app.infisical.com
INFISICAL_PROJECT_ID=your-project-id
INFISICAL_CLIENT_ID=your-client-id
INFISICAL_CLIENT_SECRET=your-client-secret

# Upstash Redis (rate limiting)
UPSTASH_REDIS_REST_URL=https://YOUR_ID.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-token

# Trigger.dev (job queuing)
TRIGGER_API_KEY=your-trigger-key
TRIGGER_API_URL=https://api.trigger.dev

# Ayrshare (social media publishing)
AYRSHARE_API_KEY=your-ayrshare-key

# Langfuse (AI observability)
LANGFUSE_SECRET_KEY=sk-lf-YOUR_KEY
LANGFUSE_PUBLIC_KEY=pk-lf-YOUR_KEY
LANGFUSE_BASEURL=https://cloud.langfuse.com

# Cloudflare (R2 storage, Workers)
CLOUDFLARE_ACCOUNT_ID=your-account-id
R2_ACCESS_KEY_ID=your-r2-key
R2_SECRET_ACCESS_KEY=your-r2-secret
R2_BUCKET_NAME=ozskr-content
